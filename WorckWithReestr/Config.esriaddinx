  <AddIn language="CLR4.5" library="WorckWithReestr.dll" namespace="WorckWithReestr">
    <ArcMap>
      <Commands>
        <Button id="TKC_WorckWithReestr_arcBtn_Open_FizLicList" class="arcBtn_Open_FizLicList" message="Работа с реестром - физические лица" caption="Физические лица" tip="Показать список физических лиц" category="TKC" image="Images\arcBtn_Open_FizLicList.png" />
        <Button id="TKC_WorckWithReestr_arcBtn_Open_JurLicList" class="arcBtn_Open_JurLicList" message="Работа с реестром - юридические лица" caption="Юридические лица" tip="Показать список юридических лиц" category="TKC" image="Images\arcBtn_Open_JurLicList.png" />
