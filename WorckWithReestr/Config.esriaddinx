        <Button id="TKC_WorckWithReestr_arcBtn_Open_ReestrVedomostey" class="arcBtn_Open_ReestrVedomostey" message="Работа с реестром - реестр ведомостей" caption="Реестр ведомостей" tip="Показать реестр ведомостей" category="TKC" image="Images\arcBtn_Open_ReestrVedomostey.png" />
      </Commands>
      <Toolbars>
        <Toolbar id="TKC_WorckWithReestr_Работа_с_реестром" caption="Работа с реестром" showInitially="true">
          <Items>
            <Button refID="TKC_WorckWithReestr_arcBtn_Open_FizLicList" />
            <Button refID="TKC_WorckWithReestr_arcBtn_Open_JurLicList" />
            <Button refID="TKC_WorckWithReestr_arcBtn_Open_TipDoc" />
            <Button refID="TKC_WorckWithReestr_arcBtn_Open_ReestrZayav" />
            <Button refID="TKC_WorckWithReestr_arcBtn_Open_ReestrVedomostey" />
